     tone(m,f4);  //32
     delay(hNote);
     noTone(m);

     tone(m,f4);  //33
     delay(qNote);
     noTone(m);
     
     delay(qNote);
     
     tone(m,f4);  //34
     delay(eNote);
     noTone(m);

     tone(m,g4);  //35
     delay(eNote);
     noTone(m);

     tone(m,a4);  //36
