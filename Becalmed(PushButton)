int m = 12; //speaker pin
int button = 3; //pushbutton
    
    //note lengths
    int qNote = 666;  //qNote is quarter note
    int hNote = qNote*2; //hNote is half note
    int wNote = qNote*4; //wNote is whole note
    int dqNote = qNote*1.5; //dotted quarter note
    int dhNote = hNote*1.5; //dotted half note
    int eNote = qNote*0.5;  //eighth note
    int sNote = eNote*0.5;  //sixteenth note
    
    //note tones
    int a3 = 220;
    int b3 = 247;
    int c4 = 262;
    int cs4 = 277;
    int d4 = 294;
    
    int e4 = 330;
    int f4 = 349;
    int g4 = 392;
    int a4 = 440;
    int b4 = 494;
    int c5 = 523;
    int ds5 = 622;
    int e5 = 659;
    int g5 = 784;
    
void setup() {
  // put your setup code here, to run once:
 pinMode(m,OUTPUT);   //sets pin as an output
 pinMode(button,INPUT_PULLUP);  //sets as a push button
}

void loop() {
  // put your main code here, to run repeatedly:
 
    
  /*
  for(int i=0; i<2; i++)
  */
  if(digitalRead(button)==LOW)
  {
     tone(m,b3);  //1
     delay(qNote);
     noTone(m);
  
     tone(m,e4);  //2
     delay(hNote);
     noTone(m);
  
     tone(m,f4);  //3
     delay(qNote);
     noTone(m);
  
     tone(m,g4);  //4
     delay(hNote);
     noTone(m);
  
     tone(m,b4);  //5
     delay(qNote);
     noTone(m);
  
     tone(m,a4);  //6
     delay(hNote);
     noTone(m);
  
     tone(m,g4);  //7
     delay(eNote);
     noTone(m);
  
     tone(m,f4);  //8
     delay(eNote);
     noTone(m);
  
     tone(m,g4);  //9
     delay(hNote);
     noTone(m);
  
     tone(m,b4);  //10
     delay(qNote);
     noTone(m);
  
     tone(m,a4);  //11
     delay(qNote);
     noTone(m);
  
     tone(m,g4);  //12
     delay(dqNote);
     noTone(m);
  
     tone(m,f4);  //13
     delay(eNote);
     noTone(m);

     tone(m,g4);  //14
     delay(hNote);
     noTone(m);
     
      tone(m,e4);  //15
     delay(qNote);
     noTone(m);

     tone(m,c4);  //16
     delay(hNote);
     noTone(m);

     tone(m,c4);  //17
     delay(hNote);
